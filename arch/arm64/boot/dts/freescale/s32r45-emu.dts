/dts-v1/;
#include "s32r45.dtsi"

/ {
	model = "Emulator for NXP S32R45";

	chosen {
		stdout-path = "serial1:115200n8";
	};

	memory_DDR0@80000000 {
		device_type = "memory";
		reg = <0 0x80000000 0 0x40000000>;
	};

	memory_DDR1@c0000000 {
		device_type = "memory";
		reg = <0 0xc0000000 0 0x40000000>;
	};
};

&timer {
	clock-frequency = <5000000>;
};
